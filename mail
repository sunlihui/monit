    set mailserver  smtp.126.com  username "tuhu_monitor" password "Tuhumima123"
    set mailserver  smtp.qq.com  username "365793275" password "1234qweR"
    set mail-format {
     from: tuhu_monitor@126.com
     subject: $SERVICE $EVENT at $DATE on $HOST
     message: Monit $ACTION $SERVICE $EVENT at $DATE on $HOST : $DESCRIPTION.
           Yours sincerely,
              Monit
      }
    set alert 365793275@qq.com

    set idfile /var/monit/id
    set eventqueue
         basedir /var/monit
